{
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    },
    "github": {
        "enabled": true,
        "silent": true
    },
    "buildCommand": "pnpm build",
    "installCommand": "pnpm install",
    "framework": "nextjs",
    "checks": {
        "wait": {
            "enabled": true,
            "requiredChecks": ["build"]
        }
    }
}
